@using ESOL_EDU.Helper
@{
    ViewBag.Title = "Dashboard";
}
@section featured {
    <header class="head">
        <div class="main-bar row">
            <div class="col-xs-6">
                <h4 class="m-t-5"><i class="fa fa-home"></i>Dashboard / <span style="color: #37506A;">@AppHelper.OrgName</span></h4>
            </div>
        </div>
    </header>
    <div class="outer">
        <div class="inner bg-container">
            <div class="row">
                <div class="col-lg-12">
                    <div id="dvNetMsg"></div>
                </div>
            </div>
            @if (AppHelper.RoleId == 1)
            {
                <div class="row">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-header bg-white">Information</div>
                            <div class="card-block" style="padding-top: 25px;">
                                <div class="col-md-3">
                                    
                                </div>
                                <div class="col-md-3">
                                    
                                </div>
                                <div class="col-md-3">
                                    
                                </div>
                                <div class="col-md-3">

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
}

